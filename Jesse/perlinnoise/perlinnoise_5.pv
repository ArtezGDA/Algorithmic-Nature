import noise
global t

def setup():
    size(200, 200)
    global t
    background("white")
    t = 10

def draw():
    global t
    stroke(0)
    t = t + 0.10
    for x in range(HEIGHT):
        y = noise.snoise2((x * 10) + t, 0.0, octaves=100)
        y *= WIDTH / 0.40
        y += WIDTH / 0.90
        line(-x, WIDTH, x, WIDTH - y)